	"scripts": {
		"codegen": "graphql-codegen --config codegen.yml",
		"dev": "next -p 4000",
		"build": "next build",
		"start": "next start",
		"analyze": "ANALYZE=true yarn build",
		"postinstall": "npx patch-package",
		"prettier-check": "prettier --config .prettierrc --check .",
		"prettier-write": "prettier --config .prettierrc --write .",
		"client:schema": "dotenv cross-var yarn apollo client:download-schema --includes=graphql --addTypename --endpoint=%WORDPRESS_API_URL%",
		"client:codegen": "yarn apollo codegen:generate --localSchemaFile=schema.json --target=typescript --includes=graphql/**/*.ts --tagName=gql --addTypename --globalTypesFile=types/graphql-global-types.ts"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn tsc && pretty-quick --staged"
		}
	},
